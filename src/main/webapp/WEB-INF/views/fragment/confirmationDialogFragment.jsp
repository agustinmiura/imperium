<div
    id="modalConfirmDialog"
    class="modal hide fade">
    <div class="modal-header">
        <h3 id="title">Confirm removal</h3>
    </div>
    <div class="modal-body">
        <p id="question">Do you want to remove the permission from the
            application</p>
    </div>
    <div class="modal-footer">
        <button
            data-option="true"
            type="button"
            class="btn btn-danger">Yes</button>
        <button
            data-option="false"
            type="button"
            class="btn btn-primary">No</button>
    </div>
</div>
